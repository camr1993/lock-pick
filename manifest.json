{
  "name": "lock-pick",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "lock-pick",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
