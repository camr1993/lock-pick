{
  "name": "lock-pick",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      
      "js": ["./dist/content.bundle.js"]
    }
  ],
  "browser_action": {
    "default_popup": "./src/popup/popup.html",
    "default_title": "lock-pick",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
